{
  "name": "vookit",
  "version": "0.8.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "post:new": "node cli/CreatePost.js",
    "user:new": "node cli/CreateUser.js",
    "upload:delete": "node cli/DeleteUpload.js",
    "dispatch:encode": "node cli/DispatchEncode.js",
    "validate": "node cli/ValidateMedia.js",
    "encode:audio": "NODE_ENV=development node cli/EncodeAudio.js",
    "encode:mp3": "NODE_ENV=development node cli/EncodeAudio.js -c mp3",
    "encode:aac": "NODE_ENV=development node cli/EncodeAudio.js -c aac_at",
    "encode:opus": "NODE_ENV=development node cli/EncodeAudio.js -c opus",
    "prisma:format": "npx prisma format",
    "prisma:reset": "npx prisma migrate reset --force",
    "prisma:migrate": "npx prisma migrate dev --name",
    "clogs:start": "node -r dotenv/config clogs/index.js",
    "clogs:start:serve": "nodemon -r dotenv/config clogs/index.js --watch ./clogs --watch ./utils --ext js,jsx --ignore **/__test__/**,./clogs/scripts/**,./clogs/styles/** --delay 5",
    "hawks:start": "node hawks/workers/launch.js",
    "hawks:start:serve": "nodemon hawks/workers/launch.js --watch ./hawks --ext js,yaml --ignore **/__test__/** --delay 5",
    "redis:start": "redis-server",
    "redis:reset": "redis-cli FLUSHALL",
    "redis:stop": "redis-cli shutdown",
    "redis:ping": "redis-cli ping",
    "webpack": "npx webpack --config ./clogs/webpack.config.js",
    "webpack:serve": "npm run webpack -- --watch",
    "webpack:build": "NODE_ENV=production npm run webpack",
    "sass": "npx sass --style=compressed --no-source-map clogs/styles/index.sass:.assets/index.css clogs/styles/form.sass:.assets/form.css",
    "sass:serve": "npx sass --watch clogs/styles/index.sass:.assets/index.css clogs/styles/form.sass:.assets/form.css",
    "openssl:init": "openssl req -x509 -newkey rsa:4096 -keyout ./.dev/key.pem -out ./.dev/cert.pem -nodes -days 30",
    "test": "jest"
  },
  "author": "mntone",
  "license": "GPLV3",
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0-beta2",
    "@prisma/client": "^4.16.1",
    "bullmq": "^4.1.0",
    "deep-freeze": "^0.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "i18n": "^0.15.1",
    "i18next": "^23.2.3",
    "i18next-fs-backend": "^2.1.5",
    "i18next-http-middleware": "^3.3.2",
    "js-yaml": "^4.1.0",
    "kiss-ffmpeg": "^0.4.2",
    "multiparty": "^4.2.3",
    "node-jsx": "^0.13.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.1",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@bull-board/api": "^5.5.3",
    "@bull-board/express": "^5.5.3",
    "@gfx/zopfli": "^1.0.15",
    "bulma": "^0.9.4",
    "bulma-switch": "^2.0.4",
    "compression-webpack-plugin": "^10.0.0",
    "eslint": "^8.43.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-jsdoc": "^46.2.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "lodash.isequal": "^4.5.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.16.1",
    "sass": "^1.63.6",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./utils/tests/setup-jest.js"
    ]
  }
}
